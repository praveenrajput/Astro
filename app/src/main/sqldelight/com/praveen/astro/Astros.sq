CREATE TABLE People (
  name TEXT PRIMARY KEY,
  craft TEXT NOT NULL,
  personBio TEXT NOT NULL,
  personImageUrl TEXT NOT NULL
);

insert:
INSERT INTO People(name, craft, personBio, personImageUrl)
VALUES ( ?, ?, ?, ?);

insertAstro:
INSERT INTO People(name, craft, personBio, personImageUrl)
VALUES ?;

deleteAll:
DELETE FROM People;

selectAstros:
SELECT *
FROM People;

selectAstro:
SELECT *
FROM People
WHERE name = ?;